        "a = int(input(\"현재년을 입력하세요: \"))\n",
        "b = int(input(\"현재월을 입력하세요: \"))\n",
        "c = int(input(\"현재일을 입력하세요: \"))\n",
        "d = int(input(\"출생년을 입력하세요: \"))\n",
        "e = int(input(\"출생월을 입력하세요: \"))\n",
        "f = int(input(\"출생일을 입력하세요: \"))\n",
        "if e < b:\n",
        "  print(\"오늘 날짜 : \",a,'년',b,'월',c,'일')\n",
        "  print(\"생년 월일 : \",d,'년',e,'월',f,'일')\n",
        "  print(\"만 나이: \",a - d,'세')\n",
        "elif e == b:\n",
        "  if f < c:\n",
        "    print(\"오늘 날짜 : \",a,'년',b,'월',c,'일')\n",
        "    print(\"생년 월일: \",d,'년',e,'월',f,'일')\n",
        "    print(\"만 나이: \",a - d,'세')\n",
        "  else:\n",
        "    print(\"오늘 날짜 : \",a,'년',b,'월',c,'일')\n",
        "  print(\"생년 월일: \",d,'년',e,'월',f,'일')\n",
        "  print(\"만 나이: \",a - d-1,'세')\n",
        "else:\n",
        "  print(\"오늘 날짜 : \",a,'년',b,'월',c,'일')\n",
        "  print(\"생년 월일: \",d,'년',e,'월',f,'일')\n",
        "  print(\"만 나이: \",a - d-1,'세')\n"
